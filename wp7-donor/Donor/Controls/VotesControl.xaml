<UserControl x:Class="Donor.Controls.VotesControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="45" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot">
        <StackPanel Orientation="Horizontal">
            <Image Source="/Donor;component/images/ic_star_pas.png" Width="30" Margin="0,0,6,0" Height="30"  Name="Star1" Opacity="1" Tap="Star1_Tap" />
            <Image Source="/Donor;component/images/ic_star_pas.png" Width="30" Margin="6,0,6,0" Height="30"  Name="Star2" Opacity="1" Tap="Star2_Tap" />
            <Image Source="/Donor;component/images/ic_star_pas.png" Width="30" Margin="6,0,6,0" Height="30"  Name="Star3" Opacity="1" Tap="Star3_Tap" />
            <Image Source="/Donor;component/images/ic_star_pas.png" Width="30" Margin="6,0,6,0" Height="30"  Name="Star4" Opacity="1" Tap="Star4_Tap" />
            <Image Source="/Donor;component/images/ic_star_pas.png" Width="30" Margin="6,0,0,0" Height="30" Name="Star5" Opacity="1" Tap="Star5_Tap" />
            
        </StackPanel>
    </Grid>
</UserControl>
